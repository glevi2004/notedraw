{
  "name": "@excalidraw/common",
  "version": "0.18.0",
  "type": "module",
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "files": [
    "dist/*"
  ],
  "description": "Excalidraw common functions, constants, etc.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "excalidraw",
    "excalidraw-utils"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "not safari < 12",
      "not kaios <= 2.5",
      "not edge < 79",
      "not chrome < 70",
      "not and_uc < 13",
      "not samsung < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": "https://github.com/excalidraw/excalidraw/issues",
  "repository": "https://github.com/excalidraw/excalidraw",
  "scripts": {
    "gen:types": "rimraf types && tsc",
    "build:esm": "rimraf dist && node ../../scripts/buildBase.js && yarn gen:types"
  },
  "dependencies": {
    "tinycolor2": "1.6.0"
  },
  "devDependencies": {
    "@types/tinycolor2": "1.4.6"
  }
}
